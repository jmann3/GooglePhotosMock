<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.jmann3.googlephotosmock"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jmann3.googlephotosmock.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.jmann3.googlephotosmock.activity.ClipChildrenActivity"/>
        <activity android:name="com.jmann3.googlephotosmock.activity.ClipPaddingActivity"/>
        <activity android:name="com.jmann3.googlephotosmock.activity.AnimationOverActivity"/>
        <activity android:name="com.jmann3.googlephotosmock.activity.GooglePhotosActivity"/>
    </application>

</manifest>